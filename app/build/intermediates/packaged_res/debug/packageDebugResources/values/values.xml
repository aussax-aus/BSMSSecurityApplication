<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="black">#000000</color>
    <color name="bsms_blue">#1E88E5</color>
    <color name="gray">#777777</color>
    <color name="ic_launcher_background">#1E88E5</color>
    <color name="purple_500">#6200EE</color>
    <color name="purple_700">#3700B3</color>
    <color name="white">#FFFFFF</color>
    <style name="MenuButton" parent="Widget.Material3.Button">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">12dp</item>
        <item name="android:layout_marginBottom">12dp</item>
        <item name="android:padding">12dp</item>
        <item name="android:textColor">@color/white</item>
        <item name="backgroundTint">@color/bsms_blue</item>
        <item name="android:textSize">16sp</item>
        <item name="iconPadding">8dp</item>
        <item name="iconTint">@color/white</item>
    </style>
    <style name="Theme.BSMS" parent="Theme.Material3.DayNight.NoActionBar"><item name="colorPrimary">#003366</item><item name="android:statusBarColor">#003366</item><item name="android:navigationBarColor">#003366</item><item name="android:windowBackground">#FFFFFFFF</item></style>
</resources>